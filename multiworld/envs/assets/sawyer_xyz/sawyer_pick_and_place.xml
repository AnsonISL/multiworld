                  size="0.02 0.02 0.02" rgba="0 0.5 1 1"
                  contype="1" conaffinity="1" friction="1 0.1 0.02" condim="3" 
            />
            
        </body>
        

        <site name="goal" pos="0.3 0.9 0.02" size="0.02"
              rgba="1 0.5 0.5 0.5"/>
       
    </worldbody>


    <sensor>
      
        <touch name='rightclaw_sensor' site='rightEndEffector'/>
        <touch name='leftclaw_sensor' site='leftEndEffector'/>
   
    </sensor>
    <actuator>
        <position ctrllimited="true" ctrlrange="-1 1" joint="r_close" kp="1000"  user="1"/>
        <position ctrllimited="true" ctrlrange="-1 1" joint="l_close" kp="1000"  user="1"/>


        
    </actuator>

