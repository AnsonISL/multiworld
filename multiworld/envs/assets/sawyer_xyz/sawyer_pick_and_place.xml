    <worldbody>
        <include file="sawyer_xyz_base.xml"></include>

        
        <body name="object0" pos="0 0.6 0" >
            <joint name="objjoint" type="free" limited='false' damping="0" armature="0" />
            <inertial pos="0 0 0" mass="0.1" diaginertia="100000 100000 100000"/>
