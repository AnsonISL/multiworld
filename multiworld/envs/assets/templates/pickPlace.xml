    <actuator>
        <position ctrllimited="true" ctrlrange="-1 1" joint="r_close" kp="1000"  user="1"/>
        <position ctrllimited="true" ctrlrange="-1 1" joint="l_close" kp="1000"  user="1"/>

    </actuator>

</mujoco>


